{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "ext-json": "*",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.2",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/adminimal_theme": "^1.6",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/blazy": "^2.13",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/cer": "^4.0@alpha",
        "drupal/chosen": "^3.0",
        "drupal/color_field": "^2.5",
        "drupal/computed_field": "^2.0",
        "drupal/config_inspector": "^1.3",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9.2",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/entity_type_clone": "^1.7",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/examples": "^3.0",
        "drupal/feeds": "^3.0@alpha",
        "drupal/field_formatter_range": "^1.4",
        "drupal/field_group": "^3.4",
        "drupal/geocoder": "^3.14",
        "drupal/geofield": "^1.44",
        "drupal/geofield_map": "^3.0",
        "drupal/gin": "^3.0@RC",
        "drupal/image_field_caption": "^1.2",
        "drupal/imagefield_slideshow": "^2.0",
        "drupal/jquery_ui": "^1.6",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/leaflet": "^10.0",
        "drupal/masquerade": "^2.0@RC",
        "drupal/media_entity_slideshow": "^2.5",
        "drupal/media_library_importer": "^1.0@beta",
        "drupal/openstreetmap": "^1.0@beta",
        "drupal/optional_end_date": "^1.3",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.11",
        "drupal/single_image_formatter": "^1.6",
        "drupal/slick": "^2.7",
        "drupal/svg_image": "^1.16",
        "drupal/token": "^1.11",
        "drupal/uikit": "dev-3.x",
        "drupal/upgrade_status": "^3.17",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_bulk_edit": "^2.8",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_geojson": "^1.2",
        "drupal/views_slideshow": "4.x-dev",
        "drupal/webprofiler": "^9.0",
        "drush/drush": "^11.0",
        "geocoder-php/arcgis-online-provider": "^4.2",
        "geocoder-php/google-maps-provider": "^4.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
            "drupal/core": {
                "#2925890 Invalid config structures can result in exceptions when saving a config entity": "https://www.drupal.org/files/issues/2021-11-29/2925890-29.patch",
                "#3145190 PHP warnings in array_flip in Drupal\\Core\\Entity\\EntityStorageBase->loadMultiple()": "https://www.drupal.org/files/issues/2022-08-17/php_warnings_in_array_flip_3145190.patch",
                "#3302838 Querying with NULL values results in warning mb_strtolower(): Passing null to parameter is deprecated": "https://www.drupal.org/files/issues/2022-08-23/3302838-13.patch"
            },
            "drupal/color_field": {
                "#3312329 PHP8.1 compatibility": "https://www.drupal.org/files/issues/2022-09-28/php81-compatibility-3312329-2.patch"
            }
        }
    },
    "require-dev": {
        "drupal/coder": "8.3.16",
        "drupal/devel": "^5.0"
    }
}
